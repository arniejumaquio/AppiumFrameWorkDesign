<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Loans Test Suite">

    <listeners>
        <listener class-name="rahulshettyacademy.Listeners"></listener>
    </listeners>

    <parameter name="webUrl" value="https://www.google.com"></parameter>
    <parameter name="mobilepackage" value="io.package"></parameter>
    <!--
        excluded mobile car loan test
    -->

    <test name="Car Loan">
        <classes>
            <class name="rahulshettyacademy.testng_demo.CarLoanTest">
            </class>
        </classes>
    </test>

    <test name="Home Loan">
        <classes>
            <class name="rahulshettyacademy.testng_demo.HomeLoanTest">
            </class>
        </classes>
    </test>

    <test name="Personal Loan">
        <classes>
            <class name="rahulshettyacademy.testng_demo.PersonalLoanTest"></class>
            <class name="rahulshettyacademy.testng_demo.PersonalLoanTest2"></class>
        </classes>
    </test>

</suite>